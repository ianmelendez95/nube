                                        "dynamodb:GetItem",
                                        "dynamodb:UpdateItem",
                                        "dynamodb:DeleteItem",
                                        "dynamodb:Query"
                                    ],
                                    "Effect": "Allow",
                                    "Resource": {
                                        "Fn::GetAtt": "ResponseTable.Arn"
                                    }
                                }
                            ],
                            "Version": "2012-10-17"
                        },
                        "PolicyName": "SQSAndDynamoPolicy"
                    }
                ],
                "RoleName": "capitalizeTwoWords-role"
            },
            "Type": "AWS::IAM::Role"
        },
